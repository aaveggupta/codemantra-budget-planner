{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/proxy/:domain/:path*",
      "destination": "https://:domain/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/abcd",
      "destination": "/ghi/",
      "permanent": true
    },
    {
      "source": "/efgh",
      "destination": "/home/",
      "permanent": true
    },
    {
      "source": "/blog2",
      "destination": "http://blog.peakflo.co/en/integrations/xero",
      "permanent": true
    }
  ]
}
