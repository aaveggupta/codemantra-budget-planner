{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/",
      "has": [
        {
          "type": "query",
          "key": "url",
          "value": "(?<paramName>.*)"
        }
      ],
      "destination": ":paramName"
    }
  ],
  "redirects": [
    {
      "source": "/abcd",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/blog2",
      "destination": "http://blog.peakflo.co/en/integrations/xero",
      "permanent": true
    }
  ]
}
