{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/specific/:path*",
      "has": [
        {
          "type": "query",
          "key": "page",
          "value": "first-(?<paramName>.*)"
        }
      ],
      "destination": "/:paramname"
    }
  ],
  "redirects": [
    {
      "source": "/abcd",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/blog2",
      "destination": "http://blog.peakflo.co/en/integrations/xero",
      "permanent": true
    }
  ]
}
